<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Exemple d'Application Web</display-name>

    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>fr.iamterror.webapp.HelloServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/index.php</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/calcul.php</url-pattern>
    </servlet-mapping>

</web-app>
